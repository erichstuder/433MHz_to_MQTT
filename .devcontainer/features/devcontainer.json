{
    "name": "features",
    "dockerComposeFile": "${localWorkspaceFolder}/features/docker-compose.yml",
    "service": "main",
    "workspaceFolder": "/home/${localEnv:USER}/features",
    "customizations": {
        "vscode": {
            "settings": {
                "terminal.integrated.shell.linux": "/bin/bash"
            },
            "extensions": [
                "rust-lang.rust-analyzer",
                "fill-labs.dependi"
            ]
        }
    },
    "remoteUser": "root"
}
